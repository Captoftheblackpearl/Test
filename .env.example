# Copy this file to .env and fill in your actual values

# Slack Configuration
# Get these from https://api.slack.com/apps
SLACK_BOT_TOKEN=xoxb-your-bot-token-here
SLACK_APP_TOKEN=xapp-your-app-token-here

# Firebase Configuration
# Get these from your Firebase project settings
__firebase_config={"apiKey":"YOUR_API_KEY","authDomain":"YOUR_AUTH_DOMAIN","projectId":"YOUR_PROJECT_ID","storageBucket":"YOUR_STORAGE_BUCKET","messagingSenderId":"YOUR_MESSAGING_SENDER_ID","appId":"YOUR_APP_ID","databaseURL":"YOUR_DATABASE_URL"}

# Firebase Auth Token (optional, for custom auth with existing users)
__initial_auth_token=

# App Configuration
__app_id=personal-bot-default
PORT=10001
